  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "name": "french-house-stack",
  "private": true,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "format": "prettier --write .",
    "gen": "plop",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky install",
    "prisma:apply-changes": "npx prisma db push",
    "prisma:reset-dev": "run-s prisma:wipe prisma:seed dev",
    "prisma:seed": "npx ts-node ./prisma/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push",
    "start": "remix-serve build",
    "test": "vitest --segfault-retry=5",
    "test:coverage": "npm run test -- --coverage",
    "test:e2e": "npx playwright test",
    "type-check": "tsc -b"
  },
  "sideEffects": false,
  "version": "1.1.0"
}
