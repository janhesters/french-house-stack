    "tailwindcss": "3.3.5",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "msw": {
    "workerDirectory": "public"
  },
  "name": "french-house-stack",
  "private": true,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "dev-with-mocks": "cross-env CLIENT_MOCKS=true SERVER_MOCKS=true npm run dev",
    "dev-with-server-mocks": "cross-env SERVER_MOCKS=true npm run dev",
    "format": "prettier --write .",
    "gen": "plop",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky install",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:reset-dev": "run-s prisma:wipe prisma:seed dev",
    "prisma:reset-dev-with-mocks": "run-s prisma:wipe prisma:seed dev-with-mocks",
    "prisma:seed": "tsx ./prisma/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:wipe": "npx prisma migrate reset --force && npx prisma db push",
    "start": "remix-serve build",
    "start-with-mocks": "cross-env CLIENT_MOCKS=true SERVER_MOCKS=true npm run start",
    "start-with-server-mocks": "cross-env SERVER_MOCKS=true npm run start",
    "test": "vitest --segfault-retry=10 --reporter=verbose",
    "test:unit": "npm run test -- '.test.{js,mjs,cjs,ts,mts,cts,jsx,tsx}'",
    "test:integration": "npm run test -- '.spec.{js,mjs,cjs,ts,mts,cts,jsx,tsx}'",
    "test:coverage": "npm run test -- --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "type-check": "tsc -b"
  },
  "sideEffects": false,
  "type": "module",
  "version": "2.0.0"
}
